{
  "name": "@entando/cms",
  "version": "0.2.226",
  "license": "LGPL-3.0",
  "description": "Entando CMS app",
  "keywords": [
    "entando",
    "cms",
    "app"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/entando/entando-cms.git"
  },
  "files": [
    "dist",
    "!src",
    "!public",
    "!node_modules"
  ],
  "main": "dist/babel.js",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@entando/apimanager": "^6.5.2",
    "@entando/pagetreeselector": "^2.0.1",
    "body-scroll-lock": "^3.1.5",
    "brace": "^0.11.1",
    "canvg": "^3.0.7",
    "react-ace": "^7.0.2",
    "react-bootstrap-typeahead": "4.2.0",
    "react-collapse": "^5.0.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-cropper": "^1.3.0",
    "react-csv": "^1.1.1",
    "react-draggable": "^3.3.0",
    "react-dropzone": "^10.2.1",
    "react-excel-workbook": "0.0.4",
    "react-html-table-to-excel": "^2.0.0",
    "react-quill-2": "^2.0.2",
    "react-roving-tabindex": "^1.0.0",
    "reactour": "^1.18.0",
    "uuid": "^8.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src/",
    "sasslint": "sass-lint -v",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "eject": "react-scripts eject",
    "coverage": "npm run test -- --coverage --watchAll=false",
    "babel": "babel src -d dist && node-sass --output-style compressed src/sass/index.scss -o dist/css",
    "prepublish": "npm run babel",
    "jenkins": "node commands/publish.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint src/ && sass-lint -v && npm run coverage"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "ignore": [
      "/**/__tests__"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src/"
          ]
        }
      ],
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ]
    ]
  },
  "sass": {
    "includePaths": [
      "node_modules/"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!src/serviceWorker.js",
      "!src/state/store.js",
      "!src/testutils/**/*.js",
      "!src/ui/**/*.js",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "eslintConfig": {
    "plugins": [
      "jest-dom",
      "testing-library"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "airbnb",
      "plugin:jest-dom/recommended",
      "plugin:testing-library/recommended",
      "plugin:testing-library/react"
    ],
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "src"
          ]
        }
      }
    },
    "rules": {
      "react/jsx-one-expression-per-line": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "jsx-a11y/label-has-associated-control": [
        2,
        {
          "controlComponents": [
            "Field"
          ],
          "depth": 3
        }
      ],
      "jsx-a11y/label-has-for": [
        2,
        {
          "components": [
            "Label"
          ],
          "required": {
            "every": [
              "nesting",
              "id"
            ]
          },
          "allowChildren": true
        }
      ],
      "import/no-extraneous-dependencies": [
        0
      ],
      "jest-dom/prefer-checked": "error",
      "jest-dom/prefer-empty": "error",
      "jest-dom/prefer-enabled-disabled": "error",
      "jest-dom/prefer-focus": "error",
      "jest-dom/prefer-required": "error",
      "jest-dom/prefer-to-have-attribute": "error",
      "jest-dom/prefer-to-have-text-content": "error",
      "testing-library/await-async-query": "error",
      "testing-library/await-async-utils": "error",
      "testing-library/await-fire-event": "error",
      "testing-library/no-await-sync-query": "error",
      "testing-library/no-dom-import": "error",
      "testing-library/no-manual-cleanup": "error",
      "testing-library/no-wait-for-empty-callback": "error",
      "testing-library/prefer-explicit-assert": "error",
      "testing-library/prefer-find-by": "error",
      "testing-library/prefer-presence-queries": "error",
      "testing-library/prefer-screen-queries": "error",
      "testing-library/prefer-wait-for": "error",
      "testing-library/no-debug": "warn"
    }
  },
  "eslintIgnore": [
    "serviceWorker.js"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.1",
    "@babel/preset-react": "^7.10.1",
    "@entando/log": "^1.0.2",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/user-event": "^11.3.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "commander": "^4.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-jest-dom": "^3.0.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-testing-library": "^3.2.2",
    "history": "^4.9.0",
    "husky": "^1.3.1",
    "jest-canvas-mock": "^2.3.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "node-sass": "^4.13.0",
    "patternfly": "3.45.0",
    "prettier": "^1.18.2",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^2.8.0",
    "react-redux": "^7.0.1",
    "react-scripts": "3.3.0",
    "redux": "^4.0.1",
    "redux-form": "^8.2.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sass-lint": "^1.12.1",
    "styled-components": "^4.4.1",
    "update-pkg-extended": "^2.2.1"
  },
  "peerDependencies": {
    "@entando/apimanager": "^6.5.2",
    "@entando/menu": "^2.0.0",
    "@entando/messages": "^1.0.4",
    "@entando/pages": "^3.2.2",
    "@entando/utils": "^2.4.0",
    "flux-standard-action": "^2.0.1",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "patternfly-react": "^1.19.1",
    "react-bootstrap": "^0.32.1",
    "react-datepicker": "^1.4.1",
    "react-router-dom": "^5.2.0"
  },
  "bugs": {
    "url": "https://github.com/entando/entando-cms/issues"
  },
  "readme": "ERROR: No README data found!",
  "_id": "@entando/cms@0.2.225",
  "homepage": "https://github.com/entando/entando-cms#readme"
}
